<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.education.supplier.dao.SupplierDao">
    <select id="getSupplierById" resultType="com.education.supplier.model.Supplier">
        select *
        from t_supplier t
        where t.supplier_token=#{supplierToken}
    </select>
</mapper>